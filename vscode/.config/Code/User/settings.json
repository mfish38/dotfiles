{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn"
  },
  "[mdc]": {
    "editor.wordWrap": "wordWrapColumn"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "catppuccin.italicComments": false,
  "catppuccin.italicKeywords": false,
  "cSpell.userWords": [
    "Dont",
    "ilike",
    "lucide",
    "msal",
    "nitropack",
    "nuxi",
    "nuxt",
    "nuxtui",
    "rrblue",
    "rrcoblue",
    "rrglobal",
    "rrgray",
    "rrgreen",
    "rrmagenta",
    "rrorange",
    "rrpurple",
    "rrred",
    "rrteal",
    "rryellow",
    "sfdark",
    "sfnuxtlayer",
    "vueuse"
  ],
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  // "editor.fontFamily": "ProggyClean Nerd Font Mono",
  // "editor.fontSize": 16,
  // "editor.lineHeight": 1.1,
  // "terminal.integrated.fontSize": 16,
  "editor.fontFamily": "GohuFont uni14 Nerd Font Mono",
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.lineNumbers": "relative",
  "editor.renderWhitespace": "none",
  "editor.rulers": [88],
  "editor.tokenColorCustomizations": {
    "[Matchalk]": {
      "textMateRules": [
        {
          "scope": [
            "variable.parameter",
            "parameter.variable",
            "meta.function.parameter variable",
            "source.rust meta.type_params.rust"
          ],
          "settings": {
            // "foreground": "#ff819f"
            "foreground": "#ee809a"
          }
        },
        {
          "scope": "meta.attribute punctuation.definition.string",
          "settings": {
            // "foreground": "#ff819f"
            "foreground": "#ee809a"
          }
        },
        {
          "scope": [
            "meta.link punctuation.definition.string",
            "meta.image punctuation.definition.string"
          ],
          "settings": {
            // "foreground": "#ff819f"
            "foreground": "#ee809a"
          }
        }
      ],
      // "variables": "#D1DED3"
      "variables": "#c7d4c9"
    }
  },
  "editor.wordWrapColumn": 88,
  "git.autofetch": true,
  "git.blame.editorDecoration.enabled": true,
  "git.confirmSync": false,
  "git.replaceTagsWhenPull": true,
  "leap.whiteSpacesOnlyMatchNewLine": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-p>"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<C-b>"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "after": ["g", "j"],
      "before": ["j"]
    },
    {
      "after": ["g", "k"],
      "before": ["k"]
    },
    {
      "after": ["g", "0"],
      "before": ["0"]
    },
    {
      "after": ["g", "^"],
      "before": ["^"]
    },
    {
      "after": ["g", "$"],
      "before": ["$"]
    },
    {
      "before": ["s"],
      "commands": ["leap.findForward"]
    },
    {
      "before": ["S"],
      "commands": ["leap.findBackward"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-p>"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<C-b>"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "after": ["g", "j"],
      "before": ["j"]
    },
    {
      "after": ["g", "k"],
      "before": ["k"]
    },
    {
      "after": ["g", "0"],
      "before": ["0"]
    },
    {
      "after": ["g", "^"],
      "before": ["^"]
    },
    {
      "after": ["g", "$"],
      "before": ["$"]
    }
  ],
  "window.menuBarVisibility": "toggle",
  "window.titleBarStyle": "native",
  "workbench.colorCustomizations": {
    "[Matchalk]": {
      "diffEditor.insertedTextBorder": "default",
      "diffEditor.removedTextBorder": "default",
      // "editor.foreground": "#D1DED3"
      "editor.foreground": "#c7d4c9"
    }
  },
  "workbench.colorTheme": "Matchalk",
  "workbench.tree.indent": 16,
  "workbench.tree.renderIndentGuides": "always"
}
